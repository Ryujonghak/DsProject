{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport User from \"../../model/user.js\";\nexport default {\n  data() {\n    return {\n      user: new User(),\n      submitted: false,\n      successful: false,\n      message: \"\"\n    };\n  },\n  computed: {\n    // loggedIn 상태 확인\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    }\n  },\n  // created() : Vue 생성되자 마자 실행되는 이벤트(화면은 생성되지 않은 상태)\n  created() {\n    if (this.loggedIn) {\n      // 로그인이 되어 있는 상태\n      this.$router.push(\"/\");\n    }\n  },\n  methods: {\n    // 회원가입 버튼 클릭시 실행되는 함수\n    handleRegister() {\n      this.message = \"\";\n      this.submitted = true;\n      this.$validator.validateAll().then(isValid => {\n        if (!isValid) {\n          return; // 함수 탈출(break)\n        }\n\n        this.$store.dispatch(\"auth/register\", this.user).then(response => {\n          this.message = response.message;\n          this.successful = true;\n          alert(\"회원가입이 완료되었습니다. 홈으로 이동됩니다\");\n          this.user = new User();\n          this.$router.push(\"/\");\n        }).catch(error => {\n          this.successful = false;\n          this.message = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n        });\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAuKA;AAEA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACAC;MACA;IACA;EACA;EACA;EACAC;IACA;MACA;MACA;IACA;EACA;EACAC;IACA;IACAC;MACA;MACA;MACA;QACA;UACA;QACA;;QACA,iDACAC;UACA;UACA;UACAC;UACA;UACA;QACA,GACAC;UACA;UACA,eACAC,kBACAA,uBACAA,+BACAA,iBACAA;QACA;MACA;IACA;EACA;AACA","names":["data","user","submitted","successful","message","computed","loggedIn","created","methods","handleRegister","then","alert","catch","error"],"sourceRoot":"src/components/signup","sources":["SignupView.vue"],"sourcesContent":["<template>\r\n  <div class=\"noticelist\" style=\"background: black\">\r\n    <div class=\"hero common-hero\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"hero-ct\">\r\n              <h1>SIGN UP 회원가입</h1>\r\n              <ul class=\"breadcumb\">\r\n                <li class=\"active\"><router-link to=\"/\">Home</router-link></li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!--회원가입 부분-->\r\n    <form class=\"signup-wrap container\"  @submit.prevent=\"handleRegister\">\r\n      <div  v-if=\"!successful\">\r\n        <!--아이디,비번,비번재확인-->\r\n        <h3>아이디</h3>\r\n        <span class=\"signup-input\">\r\n          <input \r\n          id=\"signup-id\" \r\n          type=\"text\"\r\n          v-model=\"user.username\"\r\n          name=\"username\" />\r\n          <span class=\"signup-at\"></span>\r\n        </span>\r\n\r\n        <h3>비밀번호</h3>\r\n        <span class=\"signup-input\">\r\n          <input\r\n          v-model=\"user.password\"\r\n          placeholder=\"6자리 이상 숫자를 입력하세요.\"\r\n          id=\"signup-pw\" type=\"password\" />\r\n          <span class=\"pw-lock\"></span>\r\n        </span>\r\n\r\n        <h3>비밀번호 재확인</h3>\r\n        <span class=\"signup-input\">\r\n          <input id=\"signup-pww\" type=\"password\" />\r\n          <span class=\"pww-lock\"></span>\r\n        </span>\r\n      </div>\r\n\r\n      <div style=\"margin-top: 35px\">\r\n        <!--이름,생년월일,이메일-->\r\n        <h3>이름</h3>\r\n        <span class=\"signup-input\">\r\n          <input id=\"signup-name\" type=\"text\" v-model=\"user.name\"/>\r\n        </span>\r\n\r\n        <h3>생년월일</h3>\r\n        <span style=\"display: flex\">\r\n          <span class=\"signup-input-birth\">\r\n            <input id=\"signup-birth-yy\" type=\"text\" placeholder=\"년(4자)\" \r\n            v-model=\"user.year\"/>\r\n          </span>\r\n          <span class=\"signup-input-birth\" style=\"margin-left: 10px\">\r\n            <select\r\n              id=\"signup-birth-mm\"\r\n              class=\"selectbox\"\r\n              name=\"month\"\r\n              onchange=\"\"\r\n              v-model=\"user.month\"\r\n            >\r\n              <option value=\"month\" selected>월</option>\r\n              <option value=\"1\">1</option>\r\n              <option value=\"2\">2</option>\r\n              <option value=\"3\">3</option>\r\n              <option value=\"4\">4</option>\r\n              <option value=\"5\">5</option>\r\n              <option value=\"6\">6</option>\r\n              <option value=\"7\">7</option>\r\n              <option value=\"8\">8</option>\r\n              <option value=\"9\">9</option>\r\n              <option value=\"10\">10</option>\r\n              <option value=\"11\">11</option>\r\n              <option value=\"12\">12</option>\r\n            </select>\r\n          </span>\r\n          <span class=\"signup-input-birth\" style=\"margin-left: 10px\">\r\n            <input id=\"signup-birth-dd\" type=\"text\" placeholder=\"일\"\r\n            v-model=\"user.day\" />\r\n          </span>\r\n        </span>\r\n\r\n        <span class=\"choice\">\r\n          <h3>이메일</h3>\r\n          <span></span>\r\n        </span>\r\n        <span class=\"signup-input\">\r\n          <input\r\n          v-model=\"user.email\"\r\n            id=\"signup-email\"\r\n            type=\"email\"\r\n            placeholder=\"이메일양식(@)을 입력하세요(중복 불가)\"\r\n          />\r\n        </span>\r\n      </div>\r\n\r\n      <div style=\"margin-top: 35px\">\r\n        <!--휴대전화-->\r\n        <h3>휴대전화</h3>\r\n        <div style=\"display: flex\">\r\n          <span\r\n            class=\"signup-input\"\r\n            style=\"width: 100%; margin: 10px 0px 0px 0px\"\r\n          >\r\n            <input id=\"signup-phone\" type=\"text\" placeholder=\"01012341234 형식으로 입력하세요(-제외)\" \r\n            v-model=\"user.phone\"/>\r\n          </span>\r\n        </div>\r\n      </div>\r\n\r\n      <div style=\"margin-top: 35px\">\r\n        <h3>비밀번호 찾기 질문</h3>\r\n        <span class=\"signup-input\">\r\n          <select\r\n            id=\"signup-birth-mm\"\r\n            class=\"selectbox\"\r\n            name=\"findpwd\"\r\n            style=\"width: 100%\"\r\n          >\r\n            <option value=\"ko\" selected>나의 고향은?</option>\r\n            <option value=\"ko\">어머니의 성함은?</option>\r\n            <option value=\"ko\">아버지의 성함은?</option>\r\n          </select>\r\n        </span>\r\n      </div>\r\n\r\n      <div style=\"margin-top: 35px\">\r\n        <!--비밀번호 질문 확인-->\r\n        <h3>비밀번호 찾기 정답</h3>\r\n        <div style=\"display: flex\">\r\n          <span\r\n            class=\"signup-input\"\r\n            style=\"width: 100%; margin: 10px 0px 0px 0px\"\r\n          >\r\n            <input\r\n              id=\"signup-phone\"\r\n              type=\"text\"\r\n              placeholder=\"정답을 한글로 입력하세요.\"\r\n              v-model=\"user.answer\"\r\n            />\r\n          </span>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <!--가입하기-->\r\n        <div class=\"signup-btn-wrap\">\r\n          <button id=\"signup-btn\" class=\"submit\">가입하기</button>\r\n        </div>\r\n      </div>\r\n    </form>\r\n    <div\r\n        v-if=\"message\"\r\n        class=\"alert\"\r\n        :class=\"successful ? 'alert-success' : 'alert-danger'\"\r\n      >\r\n        {{ message }}\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"../../model/user.js\";\r\n\r\nexport default {\r\n    data() {\r\n    return {\r\n      user: new User(),\r\n      submitted: false,\r\n      successful: false,\r\n      message: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    // loggedIn 상태 확인\r\n    loggedIn() {\r\n      return this.$store.state.auth.status.loggedIn;\r\n    },\r\n  },\r\n  // created() : Vue 생성되자 마자 실행되는 이벤트(화면은 생성되지 않은 상태)\r\n  created() {\r\n    if (this.loggedIn) {\r\n      // 로그인이 되어 있는 상태\r\n      this.$router.push(\"/\"); \r\n    }\r\n  },\r\n  methods: {\r\n    // 회원가입 버튼 클릭시 실행되는 함수\r\n    handleRegister() {\r\n      this.message = \"\";\r\n      this.submitted = true; \r\n      this.$validator.validateAll().then((isValid) => {\r\n        if (!isValid) {\r\n          return; // 함수 탈출(break)\r\n        }\r\n        this.$store.dispatch(\"auth/register\", this.user)\r\n          .then((response) => {\r\n            this.message = response.message;\r\n            this.successful = true;\r\n            alert(\"회원가입이 완료되었습니다. 홈으로 이동됩니다\")\r\n            this.user = new User();\r\n            this.$router.push(\"/\");\r\n          })\r\n          .catch((error) => {\r\n            this.successful = false; \r\n            this.message =\r\n              (error.response &&\r\n                error.response.data &&\r\n                error.response.data.message) ||\r\n              error.message ||\r\n              error.toString();\r\n          });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\nbody {\r\n  background-color: #f5f6f7;\r\n}\r\nselect {\r\n  width: 85px;\r\n  height: 25px;\r\n  outline: none;\r\n  background: #f5f6f7;\r\n  border: 1px solid #999;\r\n}\r\na {\r\n  color: black;\r\n  text-decoration: none;\r\n}\r\n.main {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\ninput {\r\n  cursor: pointer;\r\n}\r\n.main-signup {\r\n  text-align: center;\r\n  width: 460px;\r\n  margin: auto;\r\n}\r\nh3 {\r\n  margin: 19px 0px 8px;\r\n  text-align: left;\r\n  font-size: 16px;\r\n  color: #dadada;\r\n}\r\n.signup-input {\r\n  display: flex;\r\n  /* margin: 0px 10px 8px 10px; */\r\n  padding: 10px;\r\n  border: solid 1px #dadada;\r\n  background: #fff;\r\n  cursor: pointer;\r\n}\r\n.signup-input-c {\r\n  display: flex;\r\n  /* margin: 0px 10px 8px 10px; */\r\n  padding: 10px;\r\n  border: solid 1px #dadada;\r\n  background: #f5f6f7;\r\n  cursor: pointer;\r\n}\r\n.signup-input-c input {\r\n  background: #f5f6f7;\r\n}\r\n#signup-id,\r\n#signup-pw,\r\n#signup-pww {\r\n  height: 29px;\r\n  border: none;\r\n  outline: none;\r\n  width: 100%;\r\n}\r\n.signup-at {\r\n  color: rgb(150, 150, 150);\r\n  font-size: 15px;\r\n  font-family: Dotum, \"돋움\", Helvetica, sans-serif;\r\n  margin-top: 8px;\r\n}\r\n.pw-lock {\r\n  /* content: ''; */\r\n  /* display: inline-block; */\r\n  top: 50%;\r\n  right: 13px;\r\n  width: 24px;\r\n  height: 24px;\r\n  margin-top: 5px;\r\n  background-image: url(https://static.nid.naver.com/images/ui/join/m_icon_pw_step.png);\r\n  background-size: 125px 75px;\r\n  cursor: pointer;\r\n}\r\n.pww-lock {\r\n  /* content: ''; */\r\n  /* display: inline-block; */\r\n  top: 50%;\r\n  right: 13px;\r\n  width: 24px;\r\n  height: 24px;\r\n  margin-top: 5px;\r\n  background-image: url(https://static.nid.naver.com/images/ui/join/m_icon_pw_step.png);\r\n  background-size: 125px 75px;\r\n  cursor: pointer;\r\n}\r\n\r\n.signup-input-birth {\r\n  display: block;\r\n  position: relative;\r\n  width: 100%;\r\n  height: 51px;\r\n  border: solid 1px #dadada;\r\n  padding: 10px;\r\n  background: #fff;\r\n  box-sizing: border-box;\r\n}\r\n#signup-birth-yy,\r\n#signup-birth-mm,\r\n#signup-birth-dd {\r\n  width: 113px;\r\n  height: 29px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n#signup-name,\r\n#signup-email,\r\n#signup-country,\r\n#signup-phone,\r\n#signup-cnum {\r\n  width: 100%;\r\n  height: 29px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n// #signup-gender {\r\n//   background-color: white;\r\n// }\r\n// #signup-country {\r\n//   background-color: white;\r\n// }\r\n#signup-birth-mm {\r\n  background-color: white;\r\n}\r\n/*본인 확인 이메일*/\r\n.choice {\r\n  display: flex;\r\n}\r\n/*회원가입버튼박스*/\r\n.signup-btn-wrap {\r\n  height: 52px;\r\n  line-height: 55px;\r\n  margin: 10px 0px 50px 0px;\r\n  margin-top: 10%;\r\n  border: solid 1px rgba(0, 0, 0, 0.1);\r\n  background-color: #8813b6;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n/*회원가입버튼*/\r\n#signup-btn {\r\n  display: inline;\r\n  margin-left: 40%;\r\n  background-color: #8813b6;\r\n  border: none;\r\n  color: #fff;\r\n  font-size: 20px;\r\n  outline: none;\r\n  cursor: pointer;\r\n}\r\n.signup-wrap {\r\n  width: 30%;\r\n  margin: 0 auto;\r\n}\r\n.alert{\r\n  width: 28%;\r\n  margin: 0 auto;\r\n}\r\n.common-hero {\r\n  height: 385px;\r\n  /* // background: url(\"../images/uploads/user-hero-bg.jpg\") no-repeat; */\r\n  background: url(\"../../assets/images_kang/Components/common/Navcom/back-img-test8.png\") no-repeat !important;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}