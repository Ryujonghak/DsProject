      });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AA0SA;AACA;EACAA;IACA;MACA;MACAC;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;QAAA;QACAC;MACA;IACA;EACA;;EACAC;IACA;IACAC;MACA;MACA;QACAC;MACA;QACA;UACAjB;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;UACAC;QACA;QACAI,8BACAC;UACA;UACAC;UACAH;UACAA;UACA;QACA,GACAI;UACAD;QACA;MACA;IACA;EACA;AACA","names":["data","movie","id","moviecd","opendt","watchgradenm","movienm","plot","showtm","genrenm","directors","actor","posterurln","prdtyear","prdtstatnm","raiting","utubeurl","methods","saveMovie","alert","MovieDataService","then","console","catch"],"sourceRoot":"src/components/admin","sources":["AddMovie.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"hero user-hero\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"hero-ct\">\r\n              <h1>ADMIN PAGE</h1>\r\n              <h4 style=\"color: aliceblue\">영화등록 페이지</h4>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"page-single\">\r\n      <div class=\"container\">\r\n        <div class=\"row ipad-width2\"></div>\r\n        <!--  영화등록 테이블 시작 -->\r\n        <div class=\"container\">\r\n          <table class=\"AddMovieBox\" style=\"margin-top: 5%\">\r\n            <colgroup>\r\n              <!-- <col style=\"width: 5%\" /> -->\r\n              <col style=\"width: 15%\" />\r\n              <col style=\"width: 10%\" />\r\n              <col style=\"width: auto\" />\r\n              <col style=\"width: 15%\" />\r\n              <col style=\"width: 15%\" />\r\n            </colgroup>\r\n            <tbody>\r\n              <!-- 첫번째줄 -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"name\">영화이름</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    id=\"name\"\r\n                    class=\"input-text boxing\"\r\n                    v-model=\"movie.movienm\"\r\n                  />\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">ID</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.id\"\r\n                  />\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">장르</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.genrenm\"\r\n                  />\r\n                </td>\r\n              </tr>\r\n              <!-- 첫번째줄 끝 -->\r\n              <!-- 두번째줄 -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">러닝타임</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.showtm\"\r\n                  />\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">감독</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.directors\"\r\n                  />\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">캐스트</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.actor\"\r\n                  />\r\n                </td>\r\n              </tr>\r\n              <!-- 두번째줄 끝 -->\r\n              <!-- 세번째줄 -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"textarea\">줄거리</label>\r\n                </th>\r\n                <td colspan=\"8\">\r\n                  <div class=\"textarea\">\r\n                    <textarea\r\n                      id=\"textarea\"\r\n                      name=\"custInqCn\"\r\n                      rows=\"5\"\r\n                      cols=\"30\"\r\n                      title=\"내용입력\"\r\n                      class=\"input-textarea boxing\"\r\n                      placeholder=\"내용을 입력해주세요.\"\r\n                      v-model=\"movie.plot\"\r\n                    ></textarea>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n              <!-- 세번째줄 끝 -->\r\n              <!-- 네번째줄 -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">포스터 이미지</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <ul class=\"footer-button-plus\">\r\n                    <input\r\n                      type=\"text\"\r\n                      id=\"file\"\r\n                      class=\"inputfile\"\r\n                      v-model=\"movie.posterurln\"\r\n                    />\r\n                  </ul>\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">개봉일</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"qnaTitle\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.opendt\"\r\n                  />\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">관람등급</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <select name=\"관람등급\" id=\"age\" v-model=\"movie.watchgradenm\">\r\n                    <option value=\"전체관람가\">ALL</option>\r\n                    <option value=\"12세이상관람가\">12+</option>\r\n                    <option value=\"15세이상관람가\">15+</option>\r\n                    <option value=\"청소년관람불가\">19+</option>\r\n                  </select>\r\n                </td>\r\n              </tr>\r\n              <!-- 네번째줄 끝  -->\r\n              <!-- 다섯번째 줄 -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">영화코드</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <ul class=\"footer-button-plus\">\r\n                    <input\r\n                      type=\"text\"\r\n                      id=\"file\"\r\n                      class=\"inputfile\"\r\n                      v-model=\"movie.moviecd\"\r\n                    />\r\n                  </ul>\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">개봉(예정)</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <select\r\n                    name=\"개봉예정\"\r\n                    id=\"prdtstatnm\"\r\n                    v-model=\"movie.prdtstatnm\"\r\n                  >\r\n                    <option value=\"개봉\">개봉</option>\r\n                    <option value=\"개봉예정\">개봉예정</option>\r\n                  </select>\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">개봉연도</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"prdyear\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.prdtyear\"\r\n                  />\r\n                </td>\r\n              </tr>\r\n              <!-- 다섯번째 줄 끝 -->\r\n              <!-- 여섯번째 줄 : 정보를 추가로 받기위한 평점과 유튜브 주소창 (최아리 추가) -->\r\n              <tr>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">영화평점(최대10점)</label>\r\n                </th>\r\n                <td colspan=\"2\">\r\n                  <ul class=\"footer-button-plus\">\r\n                    <input\r\n                      type=\"text\"\r\n                      id=\"file\"\r\n                      class=\"inputfile\"\r\n                      v-model=\"movie.raiting\"\r\n                    />\r\n                  </ul>\r\n                </td>\r\n                <th scope=\"row\" class=\"noticelabel\">\r\n                  |\r\n                  <label for=\"noticeTitle\">유튜브 예고편 주소</label>\r\n                </th>\r\n                <td colspan=\"5\">\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"prdyear\"\r\n                    class=\"boxing input-text\"\r\n                    maxlength=\"100\"\r\n                    v-model=\"movie.utubeurl\"\r\n                  />\r\n                </td>\r\n              </tr>\r\n              <div\r\n                style=\"\r\n                  width: 300%;\r\n                  margin-top: 5%;\r\n                  margin-left: 455%;\r\n                  color: #abb7c4;\r\n                  font-size: small;\r\n                \"\r\n              >\r\n                <h6>*유튜브 주소 예시: https://youtu.be/dKWbgcKkn20</h6>\r\n              </div>\r\n              <!-- 여섯번째 줄 끝 -->\r\n            </tbody>\r\n          </table>\r\n          <div class=\"search\">\r\n            <button\r\n              type=\"submit\"\r\n              class=\"regbtn\"\r\n              style=\"float: right\"\r\n              @click=\"saveMovie()\"\r\n            >\r\n              등록하기\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <!-- 끝 -->\r\n      </div>\r\n    </div>\r\n    <!-- TODO: 탑버튼 추가_정주희 -->\r\n    <a class=\"topbutton\" href=\"#\">\r\n      <img src=\"@/assets/images_jung/iconUp_48.png\" />\r\n    </a>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MovieDataService from \"@/services/MovieDataService\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      // imgURL: \"\",\r\n      movie: {\r\n        id: null, //키값\r\n        moviecd: \"\", //영화코드\r\n        opendt: \"\", //개봉날짜\r\n        watchgradenm: \"전체관람가\", //관람등급\r\n        movienm: \"\", //영화이름\r\n        plot: \"\", //영화줄거리\r\n        showtm: \"\", //러닝타임\r\n        genrenm: \"\", //장르\r\n        directors: \"\", //감독\r\n        actor: \"\", //출연진\r\n        posterurln: \"\", //포스터\r\n        prdtyear: \"\", // 제작년도\r\n        prdtstatnm: \"개봉\", // 개봉작 or 개봉예정작\r\n        raiting: null, // 영화 평점\r\n        utubeurl: \"\", // 유튜브 예고편 주소\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    //영화 등록함수\r\n    saveMovie() {\r\n      // 영화평점이 10점이 초과가 되면 넘어가지 않도록 if (최아리 추가)\r\n      if (this.movie.raiting > 10) {\r\n        alert(\"영화평점을 10점 미만으로 다시 입력하세요.\");\r\n      } else {\r\n        let data = {\r\n          id: this.movie.id,\r\n          moviecd: this.movie.moviecd,\r\n          opendt: this.movie.opendt,\r\n          watchgradenm: this.movie.watchgradenm,\r\n          movienm: this.movie.movienm,\r\n          plot: this.movie.plot,\r\n          showtm: this.movie.showtm,\r\n          genrenm: this.movie.genrenm,\r\n          directors: this.movie.directors,\r\n          actor: this.movie.actor,\r\n          posterurln: this.movie.posterurln,\r\n          prdtyear: this.movie.prdtyear,\r\n          prdtstatnm: this.movie.prdtstatnm,\r\n          raiting: this.movie.raiting,\r\n          utubeurl: this.movie.utubeurl,\r\n        };\r\n        MovieDataService.create(data)\r\n          .then((response) => {\r\n            this.movie.id = response.data.id;\r\n            console.log(response.data);\r\n            alert(\"등록되었습니다.\");\r\n            alert(this.movie.raiting);\r\n            this.$router.push(\"/movie-admin\");\r\n          })\r\n          .catch((e) => {\r\n            console.log(e);\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.AddMovieBox {\r\n  background: inherit;\r\n  color: aliceblue;\r\n}\r\nth,\r\ntd {\r\n  border-bottom: 2px solid aliceblue;\r\n  padding: 10p