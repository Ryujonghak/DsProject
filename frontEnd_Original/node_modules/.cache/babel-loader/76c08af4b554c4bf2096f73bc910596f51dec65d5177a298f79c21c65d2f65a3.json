iv>\r\n    </div>\r\n    <div class=\"page-single\">\r\n      <div class=\"container\">\r\n        <div class=\"row ipad-width2\">\r\n          <!-- <!— 왼쪽 메뉴바 시작 —> -->\r\n          <div class=\"col-md-3 col-sm-12 col-xs-12\">\r\n            <div class=\"user-information\">\r\n              <div class=\"user-fav\">\r\n                <p>관리자 목록</p>\r\n                <ul>\r\n                  <li class=\"active\">\r\n                    <router-link to=\"/userInfoAdmin\">회원관리</router-link>\r\n                  </li>\r\n                  <li>\r\n                    <a href=\"#\"></a>\r\n                    <a\r\n                      class=\"btn btn-default dropdown-toggle\"\r\n                      data-toggle=\"dropdown\"\r\n                      @click=\"boardclick\"\r\n                    >\r\n                      게시판관리\r\n                      <i class=\"fa fa-angle-down\" aria-hidden=\"true\"></i>\r\n                    </a>\r\n                    <ul class=\"dropdown\" v-show=\"board\">\r\n                      <li>\r\n                        <router-link to=\"/board-admin\"\r\n                          >공지사항 관리</router-link\r\n                        >\r\n                      </li>\r\n                      <li>\r\n                        <router-link to=\"/movie-admin\">영화 관리</router-link>\r\n                      </li>\r\n                      <li>\r\n                        <router-link to=\"/review-admin\">리뷰관리</router-link>\r\n                      </li>\r\n                      <li>\r\n                        <router-link to=\"/qna-admin\">QnA 답변관리</router-link>\r\n                      </li>\r\n                    </ul>\r\n                  </li>\r\n                  <li>\r\n                    <router-link to=\"/schedule-admin\">스케쥴 관리</router-link>\r\n                  </li>\r\n                  <li>\r\n                    <router-link to=\"/payment-admin\">예매 내역</router-link>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n              <div class=\"user-fav\">\r\n                <ul>\r\n                  <li><a href=\"#\" @click=\"logout\">Log out</a></li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- <!— 왼쪽 메뉴바 끝 —> -->\r\n          <!-- 수정 양식 폼 시작 -->\r\n          <div class=\"edit-form col-md-9 col-sm-12 col-xs-12\">\r\n            <h4 style=\"color: white\">ID: {{ user.id }}</h4>\r\n            <form>\r\n              <!-- 사원명 -->\r\n              <div class=\"form-group col-xs-12\">\r\n                <label for=\"username\">username</label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"username\"\r\n                  v-model=\"user.username\"\r\n                />\r\n              </div>\r\n\r\n              <!-- 입사일 -->\r\n              <div class=\"mt-3 form-group col-xs-12\">\r\n                <label for=\"answer\">answer</label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"answer\"\r\n                  v-model=\"user.answer\"\r\n                />\r\n              </div>\r\n              <!-- 월급여 -->\r\n              <div\r\n                class=\"mt-3 form-group col-xs-12\"\r\n                style=\"padding-left: 0%; padding-right: 0%\"\r\n              >\r\n                <label class=\"col-xs-12\">생년월일</label>\r\n                <div class=\"col-xs-4\">\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"year\"\r\n                    v-model=\"user.year\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-xs-4\">\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"year\"\r\n                    v-model=\"user.month\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-xs-4\">\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"year\"\r\n                    v-model=\"user.day\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <!-- 인센티브 -->\r\n              <div class=\"mt-3 form-group col-xs-12\">\r\n                <label for=\"email\">email</label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"email\"\r\n                  v-model=\"user.email\"\r\n                />\r\n              </div>\r\n              <!-- 부서번호 -->\r\n              <div class=\"mt-3 form-group col-xs-12\">\r\n                <label for=\"phone\">phone</label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  id=\"phone\"\r\n                  v-model=\"user.phone\"\r\n                />\r\n              </div>\r\n            </form>\r\n\r\n            <div>\r\n              <a class=\"editbtn col-xs-12\" @click=\"updateUser(user.id, changePwd, user)\">수정</a>\r\n              <!-- <button class=\"mt-3 btn btn-danger\" @click=\"deleteEmp\">\r\n                Delete\r\n              </button>\r\n\r\n              <button\r\n                type=\"submit\"\r\n                class=\"ms-3 mt-3 btn btn-success\"\r\n                @click=\"updateEmp\"\r\n              >\r\n                Update\r\n              </button> -->\r\n              <p>{{ message }}</p>\r\n            </div>\r\n          </div>\r\n          <!-- 수정 양식 폼 끝 -->\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- TODO: 탑버튼 추가_정주희 -->\r\n    <a class=\"topbutton\" href=\"#\">\r\n      <img src=\"@/assets/images_jung/iconUp_48.png\"/>\r\n    </a>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserDataService from \"@/services/user.service.js\";\r\n\r\nexport default {\r\n  mounted() {\r\n    this.getUser(this.$route.params.username);\r\n  },\r\n  data() {\r\n    return {\r\n      board: false,\r\n      user: null,\r\n      message: \"\",\r\n      changePwd: false\r\n    };\r\n  },\r\n  methods: {\r\n    //왼쪽 메뉴바 slide효과\r\n    boardclick() {\r\n      this.board = !this.board;\r\n    },\r\n    logout() {\r\n      this.$store.dispatch(\"auth/logout\");\r\n      this.$router.push(\"/\");\r\n    },\r\n\r\n    getUser(username) {\r\n      UserDataService.getUserUsername(username)\r\n        .then((response) => {\r\n          this.user = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    updateUser(id, changePwd, user) {\r\n      UserDataService.update(id, changePwd, user)\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          alert(\"수정되었습니다!\");\r\n\r\n          // 수정완료시 그 전 페이지로 강제 이동\r\n          this.$router.push(\"/userInfoAdmin\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          alert(\"수정실패\")\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.user-hero {\r\n  height: 385px;\r\n  background: url(\"../../assets/images_kang/Components/common/Navcom/back-img-test16.png\") no-repeat;\r\n}\r\n.editbtn {\r\n  font-family: \"Dosis\", sans-serif;\r\n  font-size: 100%;\r\n  text-align: center;\r\n  color: black;\r\n  font-weight: bold;\r\n  width: 10%;\r\n  height: 3%;\r\n  background-color: #dcf836;\r\n  padding: 1% 2%;\r\n  margin-left: 45%;\r\n  margin-top: 7%;\r\n  border-radius: 5px;\r\n}\r\n.editbtn:hover {\r\n  color: #fff;\r\n}\r\n\r\n/* 탑버튼 추가 _정주희 */\r\n.topbutton {\r\n  position: fixed;\r\n  bottom: 15px;\r\n  right: 15px;\r\n  width: 40px;\r\n  height: 40px;\r\n  z-index: 1;\r\n  opacity: 0.8;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}